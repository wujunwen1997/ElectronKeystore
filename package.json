{
  "name": "chainspay-wallet",
  "productName": "Chainspay Wallet",
  "version": "1.0.0",
  "description": "Chainspay Wallet is a batch key manager and tx singer",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "sbwdlihao",
  "repository": "https://code.aliyun.com/bitconnect/wallet.git",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "ignore": [
          ".idea",
          "src/react-umi-dva-content",
          ".*\\.wallet"
        ],
        "icon": "assets/app-icon/app"
      },
      "electronWinstallerConfig": {
        "name": "chainspay_wallet"
      },
      "electronInstallerDMG": {
        "background": "assets/dmg/background.png",
        "icon": "assets/dmg/title.icns"
      },
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "chainspaywallet"
      }
    }
  },
  "dependencies": {
    "better-sqlite3": "^5.4.0",
    "electron": "^4.1.4",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "devtron": "^1.3.0"
  }
}
